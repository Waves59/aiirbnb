<div class="row">
    <div class="list-group text-center">
        
    <% @rooms.each do |room| %>      
    
        <%= link_to room do%>
            <div class="d-flex w-100 justify-content-between">
              <h5 class="mb-1"> <%= room.listing_name %> </h5>
            </div>
            <% room.photos.each do |photo| %>
                <%= image_tag photo.image.url(), class: "avatar-large" %>
            <% end %>
        <% end %>
      
    <% end %>
     
    </div>
</div>
